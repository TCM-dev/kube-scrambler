{
  "name": "kube-scrambler",
  "version": "1.0.0",
  "description": "Fast and complete 3x3 Rubik's cube scrambler",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TCM-dev/kube-scrambler.git"
  },
  "keywords": [
    "Speedcubing",
    "Kube"
  ],
  "author": "Valentin W",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TCM-dev/kube-scrambler/issues"
  },
  "homepage": "https://github.com/TCM-dev/kube-scrambler#readme",
  "devDependencies": {
    "prettier": "^2.3.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  },
  "files": [
    "lib/**/*"
  ]
}
